                                </li>
                            </ul>
                            <table dtable options="allCrds" class="table table-bordered table-striped table-condensed  hover">
                                <thead class="">
                                    <tr>
                                        <th>#</th>
                                        <th>Card No.</th>
                                        <th>Temporary Card No.</th>
                                        <th>Account No.</th>
                                        <th>Name on Card</th>
                                        <th>Card Type</th>
                                        <th>Card Status</th>
                                        <th>Created On</th>
                                    </tr>
                                </thead>
                                <tbody class="tb-pointer"></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(function () {
        $('body').addClass('layout1menu');
        $(".mt-multiselect").each(function (index) {
            $(this).multiselect({
                nonSelectedText: $(this).attr('noneSelectText'),
                maxHeight: 250
            });
        });
    });
</script>