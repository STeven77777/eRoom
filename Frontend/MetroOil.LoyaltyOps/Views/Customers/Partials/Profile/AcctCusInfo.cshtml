@using MetroOil.LoyaltyOps.Helpers
@model MetroOil.LoyaltyOps.Models.AcctSignUpModel
@using AcctLeftMenu = Resources.AccountLeftMenu
<div class="page-content">
    <!-- BEGIN PAGE BAR -->
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <a href="/">Home</a>
                <i class="fa fa-caret-right"></i>
            </li>
            <li>
                <a href="/Customers#/">All Customers</a>
                <i class="fa fa-caret-right"></i>
            </li>
            <li>
                <a href="/Customers#/9894934943">Account Summary</a>
                <i class="fa fa-caret-right"></i>
            </li>
            <li>
                <span>Customer Details</span>
            </li>
        </ul>
    </div>
    <!-- END PAGE BAR -->
    <!-- BEGIN PAGE TITLE-->
    <div class="portlet box blue ">
        <div class="portlet-title">
            <div class="caption">@AcctLeftMenu.CusAccCusInfo</div>
        </div>
        <div class="portlet-body form">
            <form id="fullApplicationForm" class="horizontal-form form">
                <div class="form-body">
                    <!-- BEGIN BASIC INFORMATION -->
                    <div class="page-bar quick-info">
                        <ul class="page-breadcrumb">
                            <li>
                                <span class="tooltips" data-original-title="Company Name on Card">HSBC Sentul</span>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <span class="tooltips" data-original-title="Customer Account No.">9894934943</span>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <span class="tooltips" data-original-title="Program Type">Postpaid</span>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <span class="tooltips" data-original-title="Customer Class">VIP</span>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <span class="tooltips" data-original-title="Approved On">15/04/2016</span>
                            </li>
                        </ul>
                    </div>
                    <!-- END BASIC INFORMATION -->
                    <div>
                        <h4 class="form-section">Account Info</h4>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Customer Account No.</label>
                                    <input readonly="readonly" class="form-control" value="9894934943" />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Application No.</label>
                                    <input readonly="readonly" class="form-control" value="156732898" />
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <!--/row-->
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Program Type<span>(required)</span></label>
                                    <select class="form-control">
                                        <option>Postpaid</option>
                                        <option>Prepaid</option>
                                    </select>
                                </div>
                            </div>
                            <!--/span-->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Company Name on Card<span>(required)</span></label>
                                    <input class="form-control" value="HSBC Sentul" />
                                    <span class="help-info">Maximum 26 characters including space.</span>
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <!--/row-->
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Customer Class</label>
                                    <select class="form-control">
                                        <option>VIP</option>
                                        <option>Normal</option>
                                    </select>
                                </div>
                            </div>
                            <!--/span-->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Customer Category</label>
                                    <select class="form-control">
                                        <option>Corporate</option>
                                        <option></option>
                                    </select>
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <!--/row-->
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Corporate Account</label>
                                    <select class="form-control">
                                        <option>3001 HSBC Group</option>
                                        <option>3015 Maybank Group</option>
                                    </select>
                                </div>
                                <button ng-click="openCorporateAccount();" type="button" class="btn full-btn txt-white bg-blue-light1 btn-form-control">Go to corporate account</button>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Sales Territory<span>(required)</span></label>
                                    <select class="form-control">
                                        <option>Central 1</option>
                                    </select>
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <!--/row-->
